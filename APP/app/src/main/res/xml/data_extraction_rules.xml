<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- This syntax participates in the `cloud-backup` set of rules. -->
        <!-- Include this file in rule sets and use `include` and `exclude` to control data backup. -->
        <include domain="file" path="." />
        <exclude domain="file" path="no_backup/." />
    </cloud-backup>
    <device-transfer>
        <!-- This syntax participates in the `device-transfer` set of rules. -->
        <include domain="file" path="." />
        <exclude domain="file" path="no_backup/." />
    </device-transfer>
</data-extraction-rules>
